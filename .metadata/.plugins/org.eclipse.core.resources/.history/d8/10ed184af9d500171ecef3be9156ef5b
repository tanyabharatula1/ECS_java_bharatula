
public class YourInitials 
{
	public static void main(String[] args)
	{
		System.out.println("ttttt   bbbb");
		System.out.println("  t     b   b");
		System.out.println("  t     b   b");
		System.out.println("  t     bbbb");
		System.out.println("  t     b   b");
		System.out.println("  t     b   b");
		System.out.println("  t     bbbb");
	}

}	

