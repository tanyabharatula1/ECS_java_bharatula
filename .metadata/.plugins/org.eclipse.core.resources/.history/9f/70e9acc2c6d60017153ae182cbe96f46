package variables;

public class YourSchedule 
{
	public static void main( String[] args )
	{
		String Per1, Per2, Per3, Per4, Physics ,ECS ,APUSH, TA; 
		
		Per1 = "physics";
		Per2 = "ECS";
		Per3 = "APUSH";
		Per4 = "TA";
		Physics = "Corman";
		ECS = "Mauro";
		APUSH = "Spilkin";
		TA = "Mauro";
		
		System.out.println("----------------------");
		System.out.println("|Per 1 |" + Per1 + "|" + Physics);
		System.out.println("|Per 2 |" + Per2 + "    |" + ECS);
		System.out.println("|Per 3 |" + Per3 + "  |" + APUSH);
		System.out.println("|Per 4 |" + Per4 + "    |" + TA);
		System.out.println("----------------------");
		
	}
	

}
